<button onclick = "addFunction()">Hydrogen</button>


<script>
var Hydrogen = {
chemicalSymbol: 'H',
atomicNumber: 1,
molarMass: 1.00784
}
var Helium = {
chemicalSymbol = 'He',
atomicNumber: 2,
molarMass: 4.002602
}
var Lithium {
chemicalSymbol: 'Li',
atomicNumber: 3,
molarMass: 6.938
}
var additionList = [];
var displayList = [];
function addFunction(atomNum, symbol, weight) {
additionList.push(weight);
displayList.push(symbol)
}
function combineFunction(){
var sum = 0
  for (i = 0; i < additionList.length; i++) {
sum = sum + additionList[i]
  }
  return sum
}
addFunction(1, 'H', 1.00784)
console.log(displayList)
console.log(sum)
</script>
